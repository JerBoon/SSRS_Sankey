        </Field>
        <Field Name="TotOrderQty">
          <DataField>TotOrderQty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotSalesValue">
          <DataField>TotSalesValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
